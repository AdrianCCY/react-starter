{
  "name": "chat-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"pnpm dev\" \"pnpm setup-ssl\"",
    "dev": "vite --host 0.0.0.0 --port 3001",
    "setup-ssl": "local-ssl-proxy --hostname app.dev.weizhipin.com --source 9002 --target 3001",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.26.1",
    "@tanstack/router-devtools": "^1.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "^1.25.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "local-ssl-proxy": "^2.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
